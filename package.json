{
  "name": "hitlimit-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "docs",
    "benchmarks"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "test:node": "turbo run test --filter=hitlimit",
    "test:bun": "turbo run test --filter=hitlimit-bun",
    "clean": "turbo run clean",
    "typecheck": "turbo run typecheck",
    "publish:all": "pnpm -r publish --access public",
    "benchmark": "pnpm --filter hitlimit-benchmarks bench",
    "benchmark:memory": "pnpm --filter hitlimit-benchmarks bench:memory",
    "benchmark:sqlite": "pnpm --filter hitlimit-benchmarks bench:sqlite",
    "benchmark:redis": "pnpm --filter hitlimit-benchmarks bench:redis",
    "benchmark:bun": "pnpm --filter hitlimit-benchmarks bench:bun"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.34.48",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.0.0"
}
