{
  "name": "hitlimit-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "test:node": "turbo run test --filter=hitlimit",
    "test:bun": "turbo run test --filter=hitlimit-bun",
    "clean": "turbo run clean",
    "typecheck": "turbo run typecheck",
    "publish:all": "pnpm -r publish --access public"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.34.48",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.0.0"
}
